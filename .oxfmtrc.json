{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "trailingComma": "es5",
  "printWidth": 120,
  "singleQuote": true,
  "sortPackageJson": false,
  "ignorePatterns": [
    ".github/",
    ".vscode/",
    ".idea/",
    "asset/",
    "build/",
    "coverage/",
    "deploy/",
    "dist/",
    "node_modules/",
    "public/",
    "types/",
    "*.yml",
    "*.yaml",
    "*.html",
    "*.md"
  ]
}
