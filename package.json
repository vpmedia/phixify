{
  "name": "@vpmedia/phixify",
  "version": "1.6.0",
  "description": "Phaser and Pixi.js game engine asset and manifest command line generator",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/phixify#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/phixify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/phixify.git"
  },
  "keywords": [
    "phaser",
    "pixi",
    "pixi.js",
    "pixijs",
    "asset",
    "assets",
    "bundle",
    "manifest",
    "cli",
    "generator"
  ],
  "type": "module",
  "main": "./dist/phixify.js",
  "module": "./dist/phixify.js",
  "bin": {
    "phixify": "./dist/phixify.js"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist && rm -rf ./lib",
    "build": "rollup -c",
    "postbuild": "chmod +x ./dist/phixify.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "lint": "eslint --fix \"**/*.{js,jsx}\"",
    "prepack": "npm run build",
    "lefthook:install": "lefthook install",
    "lefthook:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "commander": "^11.1.0",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsdoc": "^46.9.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.5.5",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "typescript": "^5.3.3"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  }
}
