{
  "name": "@vpmedia/phixify",
  "version": "1.5.0",
  "description": "Phaser and Pixi.js game engine asset and manifest command line generator",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/phixify#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/phixify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/phixify.git"
  },
  "keywords": [
    "phaser",
    "pixi",
    "pixi.js",
    "pixijs",
    "asset",
    "assets",
    "bundle",
    "manifest",
    "cli",
    "generator"
  ],
  "type": "module",
  "main": "./dist/phixify.js",
  "module": "./dist/phixify.js",
  "bin": {
    "phixify": "./dist/phixify.js"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist && rm -rf ./lib",
    "build": "rollup -c",
    "postbuild": "chmod +x ./dist/phixify.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint-fix": "eslint --fix \"**/*.{js,jsx}\"",
    "lint-staged": "lint-staged",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "sharp": "^0.32.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^20.6.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsdoc": "^46.8.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rollup": "^3.29.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=19.0.0"
  }
}
